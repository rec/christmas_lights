{
    "driver": {
        "typename": "serial",
        "ledtype": "LPD8806",
        "c_order": "BRG",
        "gamma": [1.1, 0.5, 0]
    },

    "layout": "strip",
    "dimensions": 80,

    "animation": {
        "typename": "sequence",
        "run": {
            "fps": 30
        },
        "animations": [
            "BiblioPixelAnimations.strip.Alternates.Alternates",
            "BiblioPixelAnimations.strip.BinaryEpochClock.BEClock",
            "BiblioPixelAnimations.strip.ColorChase.ColorChase",
            "BiblioPixelAnimations.strip.ColorFade.ColorFade",
            "BiblioPixelAnimations.strip.ColorPattern.ColorPattern",
            "BiblioPixelAnimations.strip.ColorWipe.ColorWipe",
            "BiblioPixelAnimations.strip.FireFlies.FireFlies",
            "BiblioPixelAnimations.strip.HalvesRainbow.HalvesRainbow",
            "BiblioPixelAnimations.strip.LarsonScanners.LarsonRainbow",
            "BiblioPixelAnimations.strip.LarsonScanners.LarsonScanner",
            "BiblioPixelAnimations.strip.LinearRainbow.LinearRainbow",
            "BiblioPixelAnimations.strip.PartyMode.PartyMode",
            "BiblioPixelAnimations.strip.PixelPingPong.PixelPingPong",
            "BiblioPixelAnimations.strip.RGBClock.RGBClock",
            "BiblioPixelAnimations.strip.Rainbows.Rainbow",
            "BiblioPixelAnimations.strip.Rainbows.RainbowCycle",
            "BiblioPixelAnimations.strip.Searchlights.Searchlights",
            "BiblioPixelAnimations.strip.Wave.Wave",
            "BiblioPixelAnimations.strip.Wave.WaveMove",
            "BiblioPixelAnimations.strip.WhiteTwinkle.WhiteTwinkle",
            "BiblioPixelAnimations.strip.hexclock.HEXClock"
        ]
    }
}
